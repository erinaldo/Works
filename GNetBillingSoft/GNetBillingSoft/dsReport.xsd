                         generalledger AS b ON (b.RVNO = purchaseinvoice.invoiceno OR
                         b.voucherno = purchaseinvoice.invoiceno) AND b.vouchertype = 'CP' LEFT OUTER JOIN
                         generalledger AS c ON (c.RVNO = purchaseinvoice.invoiceno OR
                         c.voucherno = purchaseinvoice.invoiceno) AND c.vouchertype = 'BP'
