                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        purchaseinvoice.invoiceno, purchaseinvoice.suplierName, SUM(DISTINCT purchasedetails.price) * SUM(DISTINCT purchasedetails.quantity) AS totalamount, 
                         SUM(DISTINCT purchaseinvoice.discount) AS discount, ISNULL(SUM(DISTINCT a.Debitamt), 0) AS Netamt, ISNULL(SUM(DISTINCT b.Creditamt), 0) 
